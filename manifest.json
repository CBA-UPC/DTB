{
  "manifest_version": 2,
  "name": "Deep Tracking Blocker",
  "version": "0.8",

  "description": "Block trackers and others element of the internet that vulnerate your privacy",
  "homepage_url": "https://github.com/CBA-UPC/DTB",

  "permissions": ["webRequest","webRequestBlocking","<all_urls>","activeTab", "storage"],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",


  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },

  "browser_action": {
      "default_icon": "icons/icon48.png",
     "default_popup": "extension_popup.html"
 },


   "background": {
        "scripts": ["resource_scripts/tf.min.js","resource_scripts/jquery-3.5.1.min.js","background.js"]
    }
}
